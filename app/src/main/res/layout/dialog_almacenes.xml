<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:text="@string/select_almacen"
        android:layout_width="match_parent"
        android:textAlignment="center"
        android:layout_margin="@dimen/dialog_select_almacen_margin"
        android:layout_height="wrap_content"/>
    <Spinner
        android:id="@+id/spn_almacenes"
        android:layout_marginLeft="@dimen/dialog_select_almacen_margin"
        android:layout_marginRight="@dimen/dialog_select_almacen_margin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</LinearLayout>
